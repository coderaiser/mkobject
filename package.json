{
  "name": "mkobject",
  "version": "1.0.2",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "make object from keys and values",
  "homepage": "http://github.com/coderaiser/mkobject",
  "keywords": [
    "array",
    "object",
    "key",
    "value"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/mkobject.git"
  },
  "scripts": {
    "test": "tape test/*.js",
    "lint": "eslint lib test",
    "watch:test": "npm run watcher -- \"npm test\"",
    "watcher": "nodemon -w lib -w test -x",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "wisdom": "npm run build",
    "6to5": "buble lib -o legacy",
    "build": "redrun 6to5 legacy",
    "legacy": "echo \"module.exports = require('./mkobject');\" > legacy/index.js"
  },
  "dependencies": {
    "currify": "^3.0.0"
  },
  "license": "MIT",
  "main": "lib/mkobject.js",
  "devDependencies": {
    "buble": "^0.19.2",
    "coveralls": "^3.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-node": "^7.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^13.0.1",
    "redrun": "^6.0.0",
    "tape": "^4.2.0"
  }
}
